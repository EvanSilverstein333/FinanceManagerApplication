@model FinanceManager.Contract.Dto.FinancialTransactionDto
@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.HiddenFor(t => t.AccountId)
    @Html.HiddenFor(t => t.RowVersion)
    <div class="form-horizontal">

        <div>
            @Html.Partial("_TransactionWriteView", Model)
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "Index", new { accountId = Model.AccountId })
</div>

